{
    "name": "contextify/laravel-resource-optimizer",
    "description": "A Laravel package that optimizes API resources with N+1 query detection, performance monitoring, smart caching, and development tools",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "resource",
        "optimization",
        "performance",
        "n+1",
        "query-detection",
        "caching",
        "monitoring",
        "api"
    ],
    "authors": [
        {
            "name": "Dulitha Rajapaksha",
            "email": "dulithamahishka94@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Contextify\\LaravelResourceOptimizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contextify\\LaravelResourceOptimizer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Contextify\\LaravelResourceOptimizer\\ResourceOptimizerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
