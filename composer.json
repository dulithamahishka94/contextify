{
    "name": "contextify/laravel-resource-context",
    "description": "A Laravel package that allows nested resources to access parent resource attributes without additional database queries",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "resource",
        "context",
        "nested",
        "optimization"
    ],
    "authors": [
        {
            "name": "Contextify",
            "email": "dulithamahishka94@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Contextify\\LaravelResourceContext\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contextify\\LaravelResourceContext\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Contextify\\LaravelResourceContext\\ResourceContextServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
